{
  "manifest_version": 2,
  "version": "0.0.2",
  "minimum_chrome_version": "22.0",

  "name": "Noplay",
  "short_name": "Noplay",
  "description": "Video autoplay blocked for good",
  "author": "Dorian Camilleri",
  "homepage_url": "https://github.com/dcamilleri/noplay",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "noplay.js"
      ]
    }
  ]
}